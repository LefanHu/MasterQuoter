<!DOCTYPE html>
<html lang='en'>

<head>
  <meta charset='UTF-8'>
  <title>Master Quoter home </title>
  <link rel="stylesheet" href="styles.css">
</head>


<body style= "background-color: #2c2f33">
  <div class="container">
    <div class="nav-wrapper">
      <div class="left-side">
        <div class="nav-link-wrapper active-nav-link">
          <a href="/">Home</a>
        </div>

        <div class="nav-link-wrapper">
          <a href="/about">About</a>
        </div>
        <div class="nav-link-wrapper">
          <a href="/commands">Commands</a>
        </div>
        <div class="nav-link-wrapper">
          <div class="special-link">
            <a href="https://discord.com/api/oauth2/authorize?client_id=795756832164413500&permissions=335936592&scope=bot" target="_blank">Invite</a>
          </div>
        </div>
      </div>

      <div class="right-side">
        <div class="brand">
          MasterQuoter
        </div>
      </div>
    </div>
    <div class="content-wrapper">

        <div class="stats-wrapper">
          <h2>All Stats</h2>
          <% if (stats.length > 0) { %>
            <% stats.forEach(stat => { %>
                <h3 class = "name"><%= stat.name %></h3>
                <p class = "value"><%= stat.value %></p>
            <% }) %>
          <% } else { %>
            <p>No stats</p>
          <% } %>


        </div>
    </div>

</body>


</html>
